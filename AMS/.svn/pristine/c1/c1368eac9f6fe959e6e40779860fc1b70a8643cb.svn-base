.cha3_btn{display:inline-block; width:80px; height:30px; border:solid 1px #397516; border-radius:13px; box-sizing:border-box; background:#4d9721; color:#fff !important;font:bold 14px/30px "돋움","굴림";text-align:center; margin:0 auto;}


/*파일첨부*/
.filebox input[type="file"] { position: absolute; width:1px; height:1px; padding: 0; margin:-1px; overflow:hidden; clip:rect(0,0,0,0); border:0;}
.filebox label {display:inline-block; width:100px; height:26px; margin-left:10px; color:#404040; font:bold 12px/26px "돋움","굴림"; text-align:center; vertical-align:middle; background:#dedede; cursor: pointer; border:1px solid #9f9f9f; border-radius:13px; box-sizing:border-box; }
.filebox a {display:inline-block; width:100px; height:26px; margin-left:10px; color:#404040; font:bold 12px/26px "돋움","굴림"; text-align:center; vertical-align:middle; background:#dedede; cursor: pointer; border:1px solid #9f9f9f; border-radius:13px; box-sizing:border-box; }
.filebox .upload-name {  display: inline-block; padding:3px 4px; font-size: inherit; font-family: inherit; line-height: normal; vertical-align: middle; background: #fff; border: 1px solid #b0b0b0; 
-webkit-appearance: none; -moz-appearance: none; appearance: none;}

/* Pagination */
.pagination {position:relative; width:100%; text-align:center; margin-top:30px; }
.pagination a {display:inline-block; text-decoration: none; color:#707070; font-size:14px; width:24px; height:24px; line-height:24px; vertical-align:middle; margin-right:2px; border:0px solid #c8c8c8;}
.pagination a.on {color:#dc662e;}
.pagination .page {background:url('../../images/btn_pagination.gif') 0 0 no-repeat; text-indent:-9999px;}
.pagination .page.prev {background-position:-24px 0; margin-right:10px;}
.pagination .page.next {background-position:-48px 0; margin-left:10px;}
.pagination .page.last {background-position:-72px 0;}


