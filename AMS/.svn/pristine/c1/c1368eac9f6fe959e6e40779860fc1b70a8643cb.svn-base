.down_btn,.up_btn,.back_btn{display:inline-block; width:70px; height:26px; border:solid 1px #555; border-radius:13px; box-sizing:border-box; background:#757575; color:#fff !important;font:bold 12px/26px "돋움","굴림"; text-align:center; }
.ilgi_add_btn{display:inline-block; margin-top:5px;width:80px; height:26px; border:solid 1px #0b6691; border-radius:13px; box-sizing:border-box; background:#0d82b9; color:#fff !important;font:bold 12px/26px "돋움","굴림";text-align:center;}
